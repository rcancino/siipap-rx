	<queryString><![CDATA[select
a.id,
a.forma_de_pago as TIPO_ID,0 as folio,a.fecha,0 as numero,l.clave,l.nombre,A.importe as total,'' as comentario,A.id as abono_id,A.tipo as ORIGEN,
S.CLAVE AS SUC_CLAVE,S.NOMBRE AS SUC_NOMBRE 
,'' AS REF
,A.importe -ifnull((SELECT SUM(P.IMPORTE) FROM aplicacion_de_cobro P WHERE P.cobro_id=A.ID ),0) AS SALDO_ANTICIPO
,(SELECT SUM(P.IMPORTE) FROM aplicacion_de_cobro P WHERE P.cobro_id=A.ID ) AS SALDO_APLICADO
,'' as banco
FROM cobro A join cliente l on (a.cliente_id=l.id) JOIN sucursal S ON (A.SUCURSAL_ID=S.ID)
where a.anticipo is true and a.id=$P{ID}]]></queryString>

